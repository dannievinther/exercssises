---
import BaseLayout from '../layouts/Flex.astro'
import Exercise from "../components/Exercise.astro";
const localData = await Astro.glob('../exercises/flex/*.md');
---
<!DOCTYPE html>
<BaseLayout title="Flex exercises">
    {localData.map((ex, i) => (
        <Exercise dataSuffix="flex" data={ex} num={i+1} all={localData.length} />
    ))}
</BaseLayout>